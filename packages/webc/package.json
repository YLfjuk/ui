{
    "name": "@ylfjuk/webc",
    "version": "0.0.3",
    "description": "The web components package",
    "author": "YLfjuk",
    "type": "module",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YLfjuk/core.git",
        "directory": "packages/webc"
    },
    "keywords": [
        "ylfjuk",
        "ui",
        "webcomponents"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
